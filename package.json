{
  "name": "sketchdev",
  "version": "0.6.1",
  "description": "Sketchapp utilities to streamline designer/developer workflow (using sketchapp tool chain).",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/BriteSnow/node-sketchdev.git"
  },
  "keyword": [
    "sketchapp",
    "sketch",
    "svg",
    "symbols",
    "sprite"
  ],
  "dependencies": {
    "@sketch-hq/sketch-file-format-ts": "^5.1.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/minimist": "^1.2.0",
    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.4.2",
    "follow-redirects": "^1.13.0",
    "fs-extra-plus": "^0.5.19",
    "lodash.debounce": "^4.0.8",
    "minimist": "^1.2.5",
    "p-spawn": "^0.5.4",
    "rimraf": "^3.0.2",
    "utils-min": "^0.1.16",
    "yauzl": "^2.10.0",
    "yauzl-promise": "^2.1.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/follow-redirects": "^1.13.0",
    "@types/mocha": "^8.0.3",
    "@types/yauzl": "^2.9.1",
    "@types/yauzl-promise": "^2.1.0",
    "mocha": "^8.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "bin": {
    "sketchdev": "./dist/bin/bin-sketchdev.js"
  },
  "files": [
    "src/",
    "dist/",
    "template-demo.html"
  ],
  "scripts": {
    "test": "TS_NODE_FILES=true ./node_modules/.bin/mocha -p ./test/tsconfig.json ./test/spec/*.spec.ts",
    "testw": "TS_NODE_FILES=true ./node_modules/.bin/mocha -p ./test/tsconfig.json ./test/spec/*.spec.ts --watch --watch-files './test/**/*.ts','./src/**/*.ts'",
    "prepack": "node_modules/.bin/rimraf ./dist && ./node_modules/.bin/tsc"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "Jeremy Chone <jeremy.chone@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BriteSnow/node-sketchdev/issues"
  }
}
